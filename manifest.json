{
  "name": "Megane",
  "description": "Removes blur and other kinds of censorship on hentai-chan and nude-moon",
  "version": "1.0.0",
  "permissions": [
    "http://*.exhentai-dono.me/*",
    "https://*.nude-moon.net/*",
    "https://*.henchan.pro/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.henchan.pro/*"],
      "js": ["chan.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://*.nude-moon.net/*"],
      "js": ["moon.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "manifest_version": 2
}